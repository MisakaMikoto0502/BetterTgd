{
    "manifest_version": 2,
    "name": "BetterTgd",
    "version": "1.0.0",
    "permissions": ["tabs"],
    "background": {
      "scripts": ["lib/core.js"],
      "persistent": true
    },
    "web_accessible_resources": ["assets/*"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}